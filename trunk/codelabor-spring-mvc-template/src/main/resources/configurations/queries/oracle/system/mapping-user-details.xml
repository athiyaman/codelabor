<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.1.dtd">
<queryservice>
	<queries>
		<query id="system.userdetails.select.authority.by.username" isDynamic="false">
			<statement>
				<![CDATA[
				SELECT /* system.userdetails.select.authority.by.username */
					AUTHORITY	
				FROM 
					USERS_AUTHORITIES_MAP
				WHERE
					USERNAME = ?
				]]>
			</statement>
			<param type="VARCHAR"/>
		</query>
	</queries>
</queryservice>
