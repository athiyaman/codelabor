<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean 
		id="system.myaccount.homeController" 
		class="org.codelabor.system.spring.controllers.ForwardController">
		<property name="successView" value="system.myaccount.home.definition"/>
	</bean>
	<bean 
		id="system.myaccount.themes.homeController" 
		class="org.codelabor.system.spring.controllers.ForwardController">
		<property name="successView" value="system.myaccount.themes.home.definition"/>
	</bean>	
</beans>
