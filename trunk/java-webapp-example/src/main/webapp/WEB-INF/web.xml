<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>springEncodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>codelaborEncodingFilter</filter-name>
    <filter-class>org.codelabor.system.web.filters.EncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>springEncodingFilter</filter-name>
    <url-pattern>/encoding/spring-filter/*.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>codelaborEncodingFilter</filter-name>
    <url-pattern>/encoding/codelabor-filter/*.jsp</url-pattern>
  </filter-mapping>
</web-app>
