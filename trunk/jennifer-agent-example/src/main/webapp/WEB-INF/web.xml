<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>jenniferFilter</filter-name>
  	<display-name>jenniferFilter</display-name>
  	<description></description>
  	<filter-class>org.codelabor.example.filter.JenniferFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>jenniferFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>

  <servlet>
  	<servlet-name>JenniferServlet</servlet-name>
  	<display-name>JenniferServlet</display-name>
  	<description></description>
  	<servlet-class>org.codelabor.example.servlet.JenniferServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JenniferServlet</servlet-name>
  	<url-pattern>/example/jenniferServlet</url-pattern>
  </servlet-mapping>

</web-app>
