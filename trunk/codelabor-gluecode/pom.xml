<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.codelabor</groupId>
  <artifactId>codelabor-gluecode</artifactId>
  <packaging>jar</packaging>
  <version>1.2.6</version>
  <name>codelabor-gluecode</name>
  <url>http://codelabor.org</url>
  <developers>
    <developer>
      <id>bomber</id>
      <name>Shin Sang Jae</name>
      <email>codelabor@gmail.com</email>
      <url>http://codelabor.org</url>
      <organization>CODELABOR</organization>
      <organizationUrl>http://codelabor.org</organizationUrl>
    </developer>
  </developers>
  <organization>
    <name>CODELABOR</name>
    <url>http://codelabor.org</url>
  </organization>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
  	<!-- java -->
  
  	<!-- codelabor system -->  	
  	
  	<!-- adobe flex -->
  	
  	<!-- softforum xecure -->
  	<dependency>
  		<groupId>javax.activation</groupId>
  		<artifactId>activation</artifactId>
  		<version>1.1</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>org.codelabor</groupId>
  		<artifactId>codelabor-system</artifactId>
  		<version>2.3.3</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>adobe</groupId>
  		<artifactId>flex-messaging-remoting</artifactId>
  		<version>3.2.0.3978</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>adobe</groupId>
  		<artifactId>flex-messaging-common</artifactId>
  		<version>3.2.0.3978</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>adobe</groupId>
  		<artifactId>flex-messaging-core</artifactId>
  		<version>3.2.0.3978</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>softforum.xecure</groupId>
  		<artifactId>xecure-web</artifactId>
  		<version>7.1.0</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  	<dependency>
  		<groupId>softforum.xecure</groupId>
  		<artifactId>xecure-db</artifactId>
  		<version>2.7</version>
  		<type>jar</type>
  		<optional>false</optional>
  	</dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>archiva.internal</id>
      <url>http://viper.codelabor.org:8080/archiva/repository/internal/</url>
    </repository>
    <site>
      <id>website.codelabor-gluecode</id>
      <url>scp://viper.codelabor.org</url>
    </site>
  </distributionManagement>
</project>