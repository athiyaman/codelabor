<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="common.idgeration">

	<select id="selectPkSequence" resultType="String">
		SELECT TO_CHAR(SYSDATE , 'YYYYMMDD')||LPAD(SQ_NEW_ID.NEXTVAL , 5 , '0') FROM DUAL
	</select>
	<select id="selectFileSequence" resultType="String">
		SELECT TO_CHAR(SYSDATE , 'YYYYMMDD')||LPAD(SQ_FILE_ID.NEXTVAL , 5 , '0') FROM DUAL
	</select>

</mapper>