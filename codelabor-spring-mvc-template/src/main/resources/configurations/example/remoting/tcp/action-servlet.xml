<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean
		id="example.remoting.tcp.baseTCPController"
		class="org.codelabor.example.remoting.tcp.spring.controllers.BaseTCPController"
		abstract="true">
		<property name="socketAdapterService" ref="example.remoting.tcp.socketAdapterService"/>
	</bean>
	<bean
 		id="example.remoting.tcp.sendController" 
		class="org.codelabor.example.remoting.tcp.spring.controllers.SendController"
		parent="example.remoting.tcp.baseTCPController">
		<property name="successView" value="example.remoting.tcp.receive.definition"/>
	</bean>
</beans>
