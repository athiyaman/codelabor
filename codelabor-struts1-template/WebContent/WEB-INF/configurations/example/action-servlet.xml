<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd 
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd			
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">
	<bean name="/example/login" class="org.codelabor.common.actions.mn.LoginAction"
		parent="baseDispatchAction">
	</bean>		
	<bean name="/example/crud" class="org.codelabor.example.actions.CrudAction"
		parent="baseDispatchAction">
		<property name="crudService" ref="example.crudService"/>
	</bean>		
	<bean name="/example/error" class="org.codelabor.example.actions.ErrorAction"
		parent="baseDispatchAction">
		<property name="errorService" ref="errorService"/>
	</bean>
	<bean name="/example/uploadFile" class="org.codelabor.system.struts.actions.UploadAction"
		parent="baseDispatchAction">
		<property name="repositoryPath" value="D:\anyframe\upload"/>
		<property name="fileManager" ref="fileManager" />
		<property name="defaultRepositoryType" value="FILE_SYSTEM" />
		<property name="uniqueFileNameGenerationService"
			ref="uniqueFileNameGenerationService" />
	</bean>
	<bean name="/example/downloadFile" class="org.codelabor.system.struts.actions.DownloadAction"
		parent="baseAction">
		<property name="fileManager" ref="fileManager"/>
	</bean>

</beans>