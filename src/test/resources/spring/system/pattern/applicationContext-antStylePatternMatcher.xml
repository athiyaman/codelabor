<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean id="antStylePatternMatcher" class="org.codelabor.system.pattern.service.AntStylePatternMatcherImpl">
		<property name="includesPatternList" ref="antStylepIncludesPatternList" />
		<property name="excludesPatternList" ref="antStylepExcludesPatternList" />
	</bean>
	
	<util:list id="antStylepIncludesPatternList">
		<value>*45</value>
		<value>ab*</value>
	</util:list>
	
	<util:list id="antStylepExcludesPatternList">
		<value>45</value>
	</util:list>
	
	
</beans>
